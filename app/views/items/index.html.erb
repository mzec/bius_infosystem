<% content_for :head do %> 
<%= render :partial => "index_menu" %>
<% end %>

<% unless @items.empty? %>

<p>
<table border='1'>
    
  <tr>
    <th>Inv#</th>
    <th>Naziv</th>
  </tr>
  
  <% for item in @items %>
    <tr>
      <td><%= link_to item.inventory_number, item_url(item) %></td>
      <td><%= link_to item.name, item_url(item) %></td>
    </tr>
  <% end %>

</table>
</p>

<% end %>
