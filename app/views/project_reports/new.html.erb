<% content_for :head do %> 
  <%= render :partial => "projects/show_menu" %>
<% end %>

<div>
  <%= error_messages_for :reports %>
</div>

<%= form_tag project_report_url(@project), { :multipart => true } do %>
  <div>
    Izvjestaj za spremanje:</br>
    <%= file_field_tag :document %>
  </div>

  <div>
    <%= submit_tag 'Spremi izvjesce' %>
  </div>
<% end %>
