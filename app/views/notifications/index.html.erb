<h1>Notifications#index</h1>
<p><%= render :partial => "index_menu" %> - <%= render :partial => "sub_menu" %></p>
<hr/>

<% unless @notifications.empty? %>

<% for notification in @notifications %>
<p> <%= link_to notification.title, notification_url(notification) %>  </p>
<p> <%= notification.body %> </p>
<hr/>
<% end %>

<% end %>
